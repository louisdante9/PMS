{
  "name": "PMS",
  "version": "1.0.0",
  "description": "This is a system that contains a list of locations and the total  number of residents broken down by gender.",
  "main": "index.js",
  "scripts": {
    "server": "nodemon app.js",
    "start": "concurrently --kill-others-on-fail \" nodemon server.js\"",
    "test": "export NODE_ENV=test && export DATABASE_URL=mongodb://localhost/pmstest && jest --detectOpenHandles --forceExit",
    "watch": "export NODE_ENV=test && export DATABASE_URL=mongodb://localhost/pmstest && jest --detectOpenHandles --forceExit --watch",
    "clear": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/louisdante9/PMS.git"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.17",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "jest": "^23.6.0"
  },
